        <Label Content="Your new password: " HorizontalAlignment="Left" Margin="40,201,0,0" VerticalAlignment="Top"/>
        <Button Content="Register" HorizontalAlignment="Left" Margin="655,344,0,0" VerticalAlignment="Top" Width="75" Click="Register_Click"/>
        <Button Content="Login" HorizontalAlignment="Left" Margin="40,32,0,0" VerticalAlignment="Top" Width="75" Click="Login_Click"/>
        <Label Name="password" Content="" HorizontalAlignment="Left" Margin="181,201,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
